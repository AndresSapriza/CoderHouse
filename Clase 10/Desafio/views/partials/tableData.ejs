<table class="table">
    <thead>
      <tr>
        <th scope="col">id</th>
        <th scope="col">Title</th>
        <th scope="col">Price</th>
        <th scope="col">Photo</th>
      </tr>
    </thead>
    <tbody>
        <% products.forEach(product => { %>
            <tr>
                <th scope="row"><%= product.id %></th>
                <td><%= product.title %></td>
                <td><%= product.price %></td>
                <td><img src=<%= product.thumbnail %>></td>
            </tr>
        <% }); %>
    </tbody>
  </table>