<div class="container">
    <div class="wrapper">
        <form name="products" class="form-product">
            <h3 class="form-product-heading">Good! Lets create a new product</h3>

            <input type="text" class="form-control" name="title" placeholder="Title" required="" autofocus="" />
            <input type="number" step="0.1" class="form-control" name="price" placeholder="Price" required=""/>
            <input type="url" class="form-control" name="thumbnail" placeholder="Thumbnail" required=""/>

            <button class="btn btn-lg btn-primary btn-block" name="Submit" value="Create" type="Submit">Create</button>
        </form>
        
    </div>
</div>